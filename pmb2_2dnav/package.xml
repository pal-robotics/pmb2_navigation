  <maintainer email="jeremie.deray@pal-robotics.com">Jeremie Deray</maintainer>
  <maintainer email="procopio.stein@pal-robotics.com">Procopio Stein</maintainer>
  <license>Apache License 2.0</license>
  <author email="enrique.fernandez@pal-robotics.com">Enrique Fernandez</author>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>pmb2_rgbd_sensors</exec_depend>
  <exec_depend>pmb2_laser_sensors</exec_depend>
  <exec_depend>pmb2_maps</exec_depend>
  <exec_depend>pal_navigation_cfg_pmb2</exec_depend>
</package>
